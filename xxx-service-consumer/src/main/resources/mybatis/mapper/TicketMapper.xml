<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.zdxue.microservice.xxx.mybatis.mapper.TicketMapper">
        
    <select id="findById" resultType="org.zdxue.microservice.xxx.model.Ticket">
        select a.id, a.user_id userId, a.code from tbl_xxx_ticket a where a.id = #{id} 
    </select>
    
</mapper>